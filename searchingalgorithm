#include<iostream>
using namespace std;
int linearsearch(int arr[], int n, int key){
     for (int i = 0; i<n; i++){
        if(arr[i] == key){
        cout<<"found that key"<<key;
        }
        else{
            cout<<"key not found";
        }
    }
}

int main() {

    int n;
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    int key;
    cout<<"enter the key";
    cin>>key;

cout<<linearsearch(arr,n,key);

   return 0;
}
